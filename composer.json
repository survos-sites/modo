{
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "survos-sites/modo",
    "description": "source for modo.survos.com, a QR-code based room app",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-sodium": "*",
        "api-platform/doctrine-orm": "^4.1.3",
        "api-platform/symfony": "^4.1.2",
        "doctrine/dbal": "^4.2.3",
        "doctrine/doctrine-bundle": "^2.14.0",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "doctrine/orm": "^3.3.2",
        "endroid/qr-code-bundle": "^6.0",
        "friendsofsymfony/jsrouting-bundle": "^3.5.2",
        "inspector-apm/inspector-symfony": "^1.8.2",
        "league/commonmark": "^2.6.1",
        "league/csv": "^9.22",
        "nelmio/cors-bundle": "^2.5",
        "paragonie/sodium_compat": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.6.1",
        "phpstan/phpdoc-parser": "^2.1",
        "survos/deployment-bundle": "^1.5.529",
        "survos/js-twig-bundle": "^1.5.529",
        "symfony/asset": "^7.2",
        "symfony/asset-mapper": "^7.2.3",
        "symfony/console": "^7.2.1",
        "symfony/doctrine-messenger": "^7.2.3",
        "symfony/dotenv": "^7.2",
        "symfony/expression-language": "^7.2",
        "symfony/flex": "^2.5",
        "symfony/form": "^7.2.4",
        "symfony/framework-bundle": "^7.2.4",
        "symfony/http-client": "^7.2.4",
        "symfony/intl": "^7.2",
        "symfony/mailer": "^7.2.3",
        "symfony/mime": "^7.2.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.2",
        "symfony/process": "^7.2.4",
        "symfony/property-access": "^7.2.3",
        "symfony/property-info": "^7.2.3",
        "symfony/runtime": "^7.2.3",
        "symfony/security-bundle": "^7.2.3",
        "symfony/serializer": "^7.2.4",
        "symfony/stimulus-bundle": "^2.23",
        "symfony/string": "^7.2",
        "symfony/translation": "^7.2.4",
        "symfony/twig-bundle": "^7.2",
        "symfony/ux-icons": "^2.23",
        "symfony/validator": "^7.2.4",
        "symfony/web-link": "^7.2",
        "symfony/yaml": "^7.2.3",
        "tacman/dictionary-bundle": "^4.0.9",
        "twig/extra-bundle": "^3.20",
        "twig/intl-extra": "^3.20",
        "twig/markdown-extra": "^3.20",
        "twig/twig": "^2.12|^3.20"
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default \"": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "jsrouting": "bin/console fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default  \""
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.2"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "phpunit/phpunit": "^9.6.22",
        "survos/maker-bundle": "^1.5.503",
        "symfony/browser-kit": "^7.2.4",
        "symfony/css-selector": "^7.2",
        "symfony/debug-bundle": "^7.2",
        "symfony/maker-bundle": "^1.62.1",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/stopwatch": "^7.2.4",
        "symfony/web-profiler-bundle": "^7.2.4"
    }
}
